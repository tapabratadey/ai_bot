<?xml version="1.0" encoding="UTF-8"?>
<aiml>
  <category>
    <pattern>*</pattern>
    <template>I have no answer for that.</template>
  </category>

  <category>
    <pattern>HI</pattern>
    <template><srai>hello</srai></template>
  </category>
  
  <category>
    <pattern>BYE</pattern>
    <template>Bye!</template>
  </category>
  
 <category>
    <pattern>HELLO</pattern>
    <template>
    Hello!
    <think><set name="topic">jobs</set></think>
    <br/>Would you want me to recommend Computer Science jobs?
    </template>
</category>
<topic name="jobs">
    <category>
        <pattern>YES</pattern>
        <that>Would you want me to recommend Computer Science jobs</that>
        <template>
            Great! I would like you to tell me a bit about yourself!
            <br/>
            <br/>Which language do you feel comfortable programming in?
            <button>
              <text>Python</text>
              <postback>PYTHON</postback>
            </button>
            <button>
              <text>Javascript</text>
              <postback>JAVASCRIPT</postback>
            </button>
            <button>
              <text>C</text>
              <postback>C</postback>
            </button>
            <button>
              <text>SQL</text>
              <postback>SQL</postback>
            </button>
            <button>
              <text>Swift</text>
              <postback>SWIFT</postback>
            </button>
        </template>
    </category>
    <category>
        <pattern>JAVASCRIPT</pattern>
        <template>You could work as a Web Developer!</template>
    </category>
    <category>
        <pattern>PYTHON</pattern>
        <template>You could work as a Data Scientist!</template>
    </category>
    <category>
        <pattern>C</pattern>
        <template>You could work as a Systems Engineer!</template>
    </category>
    <category>
        <pattern>SQL</pattern>
        <template>You could work as a Database Administrator!</template>
    </category>
    <category>
        <pattern>SWIFT</pattern>
        <template>You could work as a iOS Developer!</template>
    </category>
    
    <category>
        <pattern>NO</pattern>
        <that>Would you want me to recommend Computer Science jobs</that>
        <template>Okay bye!</template>
    </category>
 </topic>
 
 
  
</aiml>